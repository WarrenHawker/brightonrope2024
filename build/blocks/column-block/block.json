{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "brighton-rope/column-block",
  "version": "0.1.0",
  "title": "Brighton Rope Column Block",
  "category": "widgets",
  "icon": "columns",
  "description": "Custom column block for Brighton Rope theme",
  "textdomain": "brighton-rope-column",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {},
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "multiple": true,
    "innerBlocks": true,
    "anchor": true,
    "color": {
      "background": true,
      "text": true,
      "link": true
    },
    "typography": {
      "fontSize": true
    }
  }
}